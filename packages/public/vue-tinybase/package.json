{
  "name": "vue-tinybase",
  "version": "0.2.0",
  "type": "module",
  "description": "TinyBase Binding for Vue 3",
  "module": "./dist/main.mjs",
  "types": "./dist/main.d.mts",
  "exports": {
    ".": {
      "types": "./dist/main.d.mts",
      "import": "./dist/main.mjs"
    },
    "./custom-store": {
      "types": "./dist/custom-store/main.d.mts",
      "import": "./dist/custom-store/main.mjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nickmessing/vue-tinybase.git",
    "directory": "packages/public/vue-tinybase"
  },
  "bugs": {
    "url": "https://github.com/nickmessing/vue-tinybase/issues"
  },
  "scripts": {
    "lint:path": "eslint --ext .ts --fix",
    "lint": "pnpm run lint:path .",
    "dev": "pkgroll --tsconfig=tsconfig.package.json --watch",
    "build": "pkgroll --tsconfig=tsconfig.package.json",
    "generate:00_generate": "tsx scripts/generate/main.ts",
    "generate:01_format": "pnpm run lint:path src/generated",
    "generate": "run-s generate:*"
  },
  "author": "Nicolai Moraru <dot.nick.dot.messing@gmail.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/generator": "^7.24.5",
    "@babel/parser": "^7.24.5",
    "@babel/traverse": "^7.24.5",
    "@babel/types": "^7.24.5",
    "@local/eslint-config-vue-tinybase": "workspace:*",
    "@tsconfig/node20": "^20.1.4",
    "@types/babel__generator": "^7.6.8",
    "@types/babel__traverse": "^7.20.5",
    "@types/node": "^20.12.7",
    "eslint": "^8.57.0",
    "mkdirp": "^3.0.1",
    "node-fetch": "^3.3.2",
    "pkgroll": "^2.0.2",
    "rimraf": "^5.0.5",
    "tsx": "^4.9.0",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@vue/reactivity": "^3.3",
    "@vue/runtime-core": "^3.3",
    "tinybase": "^4.8"
  }
}
